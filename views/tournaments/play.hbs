<h1>Helooooo, ,play with us</h1>
{{#each tournamentsList}}
<article>
  <h3>{{name}}</h3>
  <p>{{location}}</p>
  <p>{{date}}</p>
  <p>{{players.length}}/{{numberPlayers}}</p>
  <form action="/users/tournaments/{{_id}}" method="post">
    <button class="button" id="{{_id}}" >Join</button>
  </form>
</article>
{{/each}}

